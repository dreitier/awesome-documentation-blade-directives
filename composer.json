{
  "name": "dreitier/awesome-documentation-blade-directives",
  "description": "Make better developer documentations",
  "keywords": [
    "awesome-documentation-blade-directives",
    "markdown",
    "highlight",
    "highlightjs",
    "dreitier"
  ],
  "homepage": "https://dreitier.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Christopher Klein",
      "email": "ckl@dreitier.com",
      "homepage": "https://schakko.de"
    }
  ],
  "require": {
    "php": "^7.1.3|^8.0",
    "laravel/framework": "^5.7|^6.0|^7.0|^8.0",
    "scrivo/highlight.php": "v9.18.1.6",
    "jfcherng/php-diff": "^6.10"
  },
  "autoload": {
    "psr-4": {
      "Dreitier\\Documentation\\Blade\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dreitier\\Documentation\\Blade\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dreitier\\Documentation\\Blade\\BladeDirectivesServiceProvider"
      ]
    }
  }
}